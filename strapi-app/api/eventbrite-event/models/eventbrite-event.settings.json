{
  "kind": "collectionType",
  "collectionName": "eventbrite_events",
  "info": {
    "name": "Eventbrite Event",
    "description": "Events are populated from the eventbrite API any changes will be overwritten by a cron job"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Url": {
      "type": "string",
      "required": true
    },
    "ImageUrl": {
      "type": "string",
      "required": true
    },
    "EventID": {
      "type": "uid",
      "required": true
    },
    "IsCourse": {
      "type": "boolean",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "StartTime": {
      "type": "datetime",
      "required": true
    },
    "SeriesUID": {
      "type": "string"
    },
    "IsSeries": {
      "type": "boolean",
      "required": true
    }
  }
}